<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E7D27F"
    tools:context=".view.round.RoundStartActivity">

    <TextView
        android:id="@+id/roundNameTextView"
        android:layout_width="197dp"
        android:layout_height="81dp"
        android:layout_marginStart="107dp"
        android:layout_marginTop="200dp"
        android:layout_marginEnd="107dp"
        android:layout_marginBottom="108dp"
        app:layout_constraintBottom_toTopOf="@+id/startRoundButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/startRoundButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="161dp"
        android:layout_marginTop="108dp"
        android:layout_marginEnd="162dp"
        android:layout_marginBottom="294dp"
        android:text="Start"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/roundNameTextView" />
</androidx.constraintlayout.widget.ConstraintLayout>